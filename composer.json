{
  "name": "rockschtar/wordpress-notices",
  "license": "MIT",
  "description": "WordPress Must Use Plugin that helps to display notices in wordpress admin area. Developed for usage with composer based based WordPress projects (roots/bedrock or johnpbloch/wordpress).",
  "type": "wordpress-muplugin",
  "homepage": "https://github.com/rockschtar/wordpress-metarevisions",
  "authors": [
    {
      "name": "Stefan Helmer",
      "email": "rockschtar@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer" : "^3.10.1",
    "phpunit/phpunit": "8.*",
    "brain/monkey": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Rockschtar\\WordPress\\Notices\\": "src/",
      "Rockschtar\\WordPress\\Notices\\Tests\\": "tests/"
    }
  }
}
